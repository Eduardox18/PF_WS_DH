<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Fotografia"> 
    
    <insert id="guardarFoto" parameterType="servicios.pojos.Fotografia">
        INSERT INTO fotografia (foto, idReporte, fechaHora) VALUES (#{foto}, 
        #{idReporte}, #{fechaHora});
    </insert>
</mapper>